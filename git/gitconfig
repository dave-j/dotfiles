[core]
	editor = vim
[alias]
	b = branch
	co = checkout
        cp = cherry-pick
	d = diff
	dc = diff --cached
	f = fetch
	p = push
	s = status
	t = log --graph --branches --decorate
	tr = log --graph --branches --decorate --remotes
	tra = log --graph --decorate --all --remotes --branches
	tro = log --pretty=oneline --abbrev-commit --graph --decorate
	unstage = reset HEAD --
[push]
	default = simple
[include]
	path = ~/.gitconfig.local
